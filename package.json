{
  "name": "address-book",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "css-unit-converter": "^1.1.1",
    "exceljs": "^1.1.0",
    "express": "^4.16.3",
    "is-electron": "^2.1.0",
    "is-offline": "^2.1.0",
    "isvalid": "^1.6.6",
    "material-ui": "^1.0.0-beta.36",
    "material-ui-icons": "^1.0.0-beta.36",
    "mongoose": "^5.0.11",
    "node-offline-localhost": "^0.9.0",
    "node-sass-chokidar": "^1.1.2",
    "object-formdata-convertor": "^1.2.2",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-block-ui": "^1.1.1",
    "react-dom": "^16.2.0",
    "react-draggable": "^3.0.5",
    "react-multi-toggle": "^1.1.0",
    "react-print": "^1.3.1",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.1",
    "react-toastify": "2.2.1",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-form-material-ui": "^5.0.0-beta.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "wait-on": "^2.1.0"
  },
  "scripts": {
    "node-seed": "node server/seed/distance.js",
    "node-start": "node server/app.js",
    "transpile-sass": "node-sass-chokidar src/styles/sass/ -o src/styles/css/ --watch --recursive",
    "react-start": "set \"BROWSER=none\" && react-scripts start",
    "electron-start": "set \"ELECTRON_START_URL=http://127.0.0.1:3000\" && electron .",
    "start": "concurrently \"npm run node-start\" \"npm run transpile-sass\" \"npm run react-start\" \"wait-on http://127.0.0.1:3000/ && npm run electron-start\"",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "electron": "^1.8.4"
  }
}
